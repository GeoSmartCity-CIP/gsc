<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="routingPreferencesInsertRequest">
		<xs:annotation>
			<xs:documentation>Only one preference for User</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="routingPreference" type="routingPreferenceType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="routingPreferencesDeleteRequest">
		<xs:annotation>
			<xs:documentation>Delete service request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserID" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="routingPreferencesSearchRequest">
		<xs:annotation>
			<xs:documentation>search service request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserID" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="routingPreferencesInsertResponse" type="routingPreferencesInsertResponseType">
		<xs:annotation>
			<xs:documentation>Response to insert service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="routingPreferencesDeleteResponse" type="routingPreferencesDeleteResponseType">
		<xs:annotation>
			<xs:documentation>Response to delete service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="routingResponseSearchResponse" type="routingPreferencesSearchResponseType">
		<xs:annotation>
			<xs:documentation>Response to search  service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ErrorType">
		<xs:annotation>
			<xs:documentation>Error structure</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="xs:string"/>
			<xs:element name="Description" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="routingPreferencesInsertResponseType">
		<xs:sequence>
			<xs:element name="Error" type="ErrorType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="routingPreferencesSearchResponseType">
		<xs:choice>
			<xs:element name="RoutingPreference" type="routingPreferenceType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Error" type="ErrorType"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="routingPreferencesDeleteResponseType">
		<xs:sequence>
			<xs:element name="Error" type="ErrorType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="routingPreferenceType">
		<xs:sequence>
			<xs:element name="UserID" type="xs:string">
				<xs:annotation>
					<xs:documentation>User id</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaxWalkDistance" type="xs:int">
				<xs:annotation>
					<xs:documentation>Max walking distance in meter</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WalkingSpeed" type="xs:int">
				<xs:annotation>
					<xs:documentation>Walking speed. m/s</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaxBikeDistance" type="xs:int">
				<xs:annotation>
					<xs:documentation>Max biking distance in meter</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BikingSpeed" type="xs:int">
				<xs:annotation>
					<xs:documentation>Biking speed. m/s</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
